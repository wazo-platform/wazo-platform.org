{
  "name": "wazo-doc-api",
  "private": true,
  "description": "Wazo project documentation for developers",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/gatsby build --prefix-paths",
    "develop": "./node_modules/.bin/gatsby develop",
    "format": "prettier --write 'src/**/*.{js,jsx}' 'tests/src/*.js' 'gatsby-node.js'",
    "format:uc-doc": "prettier --print-width 100 --prose-wrap always --write 'content/uc-doc/**/*.md'",
    "start": "npm run develop",
    "serve": "./node_modules/.bin/gatsby serve"
  },
  "dependencies": {
    "algoliasearch": "^3.33.0",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.4.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-remark-autolink-headers": "^2.3.0",
    "gatsby-remark-emojis": "^0.4.2",
    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-source-filesystem": "^2.0.34",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-yaml": "^2.1.12",
    "graphiql": "^0.17.5",
    "js-yaml": "^3.13.1",
    "mobx": "^4.2.0",
    "react": "^16.8.6",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-in-viewport": "0.0.37",
    "react-instantsearch-dom": "^5.6.0",
    "react-markdown": "^4.0.8",
    "redoc": "^2.0.0-rc.8-1",
    "rss": "^1.2.2",
    "showdown": "^1.9.0",
    "stickyfill": "^1.1.1-strict",
    "striptags": "^3.2.0",
    "styled-components": "^4.2.0",
    "swagger-ui-react": "^3.23.11"
  },
  "devDependencies": {
    "gatsby-plugin-sass": "^2.1.0",
    "dart-sass": "^1.25.0",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wazo-platform/wazo-doc-api"
  }
}
